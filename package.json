{
  "name": "@blinkk/editor-server",
  "version": "1.0.1",
  "license": "MIT",
  "homepage": "https://blinkkcode.github.com/live-edit-server",
  "description": "Server for live editor.",
  "keywords": [
    "editor",
    "live",
    "server"
  ],
  "bugs": {
    "url": "https://github.com/blinkkcode/live-edit-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkkcode/live-edit-server.git"
  },
  "engines": {
    "node": ">=14"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "editor-dev": "./dist/src/index.js"
  },
  "files": [
    "dist/src"
  ],
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "coverage": "codecov",
    "dev": "concurrently \"npx ava --watch\" \"tsc --watch\"",
    "docs": "typedoc",
    "fix": "npx eslint --fix src/**/*.ts",
    "hosted": "nodemon ./src/server/server.ts",
    "prepublishOnly": "yarn run compile",
    "serve": "nodemon  ./src/index.ts",
    "test": "nyc ava",
    "test-ci": "nyc --reporter=lcov ava && codecov"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "ava": "^3.14.0",
    "codecov": "^3.8.1",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-sort-class-members": "^1.9.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.32",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@blinkk/editor": "^1.0.8",
    "@google-cloud/datastore": "^6.3.1",
    "@google-cloud/error-reporting": "^2.0.1",
    "@octokit/core": "^3.3.1",
    "@types/bent": "^7.3.2",
    "bent": "^7.3.12",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "isomorphic-git": "^1.8.1",
    "js-yaml": "^4.0.0"
  }
}
