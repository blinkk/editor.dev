{
  "name": "@blinkk/editor.dev",
  "version": "1.2.0",
  "license": "MIT",
  "homepage": "https://blinkk.github.com/editor.dev",
  "description": "Server for live editor.",
  "keywords": [
    "editor",
    "live",
    "server"
  ],
  "bugs": {
    "url": "https://github.com/blinkk/editor.dev/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkk/editor.dev.git"
  },
  "engines": {
    "node": ">=14"
  },
  "main": "./dist/index.js",
  "bin": {
    "editor-dev": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "npx tsc --skipLibCheck",
    "coverage": "codecov",
    "dev": "concurrently \"npx ava --watch\" \"npx tsc --watch\"",
    "docs": "npx typedoc",
    "fix": "npx eslint --fix src/ts/**/*.ts",
    "hosted": "nodemon ./src/ts/server/server.ts",
    "prepublishOnly": "npx tsc --skipLibCheck",
    "serve": "nodemon ./src/ts/index.ts",
    "test": "nyc ava",
    "test-ci": "nyc --reporter=lcov ava && codecov"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@types/cors": "^2.8.11",
    "@types/express": "^4.17.13",
    "@types/javascript-time-ago": "^2.0.2",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "ava": "^3.14.0",
    "codecov": "^3.8.2",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.9",
    "nyc": "^15.1.0",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@blinkk/editor.dev-ui": "^3.1.0",
    "@blinkk/selective-edit": "^2.0.0",
    "@google-cloud/datastore": "^6.4.4",
    "@google-cloud/error-reporting": "^2.0.2",
    "@octokit/core": "^3.5.1",
    "bent": "^7.3.12",
    "commander": "^8.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "isomorphic-git": "^1.9.1",
    "js-yaml": "^4.1.0"
  }
}
